<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<Group xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sunkur.springjavafxcontroller.screen1.Screen1Controller">
   <children>
      <StackPane prefHeight="150.0" prefWidth="200.0" style="-fx-background-color: #556699;">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
               <children>
                  <Label fx:id="screenName" text="SCREEN1" />
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                        <TextField fx:id="textField" />
                     </children>
                  </HBox>
                  <Button mnemonicParsing="false" onAction="#handleScreen2ButtonAction" text="Screen2" />
                  <Button mnemonicParsing="false" onAction="#handleScreen3ButtonAction" text="Screen3" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </StackPane>
   </children>
</Group>
